{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": "src",
    "paths": {
      "@app/*": ["app/*"],
      "@components/*": ["components/*"],
      "@pages": ["pages"]
    }
  },
  "include": ["vite.config.ts", "vite-plugins"]
}
